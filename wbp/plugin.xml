<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="wbp.ui.category"
            name="WBP">
      </category>
      <wizard
            category="wbp.ui.category"
            class="wbp.ui.ImportWARWizard"
            icon="icons/WBP.gif"
            id="wbp.ui.ImportWARWizard"
            name="WAR Binary Project">
         <description>
            Import a WAR binary project into the workspace.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="wbp.ui.PreferencePage"
            id="wbp.preferences"
            name="WBP">
      </page>
   </extension>

</plugin>
